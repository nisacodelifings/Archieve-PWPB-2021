<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ulangan_aulia">

    <application
        android:allowBackup="true"
        android:icon="@drawable/back"
        android:label="@string/app_name"
        android:roundIcon="@drawable/back"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ulangan_Aulia">
        <activity android:name=".AddNote"
            android:theme="@style/Theme.Ulangan_Aulia.NoActionBar" ></activity>
        <activity
            android:name=".Home"
            android:theme="@style/Theme.Ulangan_Aulia.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ulangan_Aulia.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>